{
  "name": "udp-hole-puncher",
  "version": "0.2.2",
  "description": "UDP hole punching library",
  "main": "index.js",
  "keywords": [
    "nat",
    "traversal",
    "udp",
    "punching"
  ],
  "author": {
    "name": "Nico Janssens",
    "email": "nico.b.janssens@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4.x.x"
  },
  "browser": {
    "dgram": "chrome-dgram",
    "winston": "./lib/logger"
  },
  "dependencies": {
    "chrome-dgram": "^3.0.0",
    "debug": "^2.2.0",
    "hat": "0.0.3",
    "merge": "^1.2.0",
    "winston": "^2.2.0",
    "winston-meta-wrapper": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "envify": "^3.4.1",
    "mocha": "^2.4.5",
    "once": "1.x",
    "publish": "^0.6.0",
    "uglify-js": "^2.7.0",
    "yargs": "^3.32.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/microminion/udp-hole-puncher-js.git"
  },
  "scripts": {
    "build": "browserify -s UdpHolePuncher -e ./ | uglifyjs -c warnings=false -m > udp_hole-puncher.min.js",
    "build-debug": "browserify -s UdpHolePuncher -e ./ > udp_hole-puncher.debug.js",
    "size": "npm run build && cat udp_hole-puncher.min.js | gzip | wc -c",
    "test-node": "./node_modules/.bin/mocha test/node.unit.js",
    "clean": "rm -f udp_hole-puncher.*.js && rm -rf node_modules",
    "2npm": "publish",
    "peer": "node examples/peer.js $"
  }
}
